{
  "name": "EXTREME TEST Theme",
  "version": "1.0.0",
  "colors": {
    "primary": "#FF0000",
    "primaryForeground": "#FFFFFF",
    "secondary": "#00FF00",
    "secondaryForeground": "#000000",
    "background": "#FF0000",
    "foreground": "#FFFFFF",
    "muted": "#2D2D3F",
    "mutedForeground": "#9CA3AF",
    "accent": "#9945FF",
    "accentForeground": "#FFFFFF",
    "border": "rgba(255, 255, 255, 0.1)",
    "success": "#10B981",
    "error": "#EF4444",
    "warning": "#F59E0B",
    "info": "#00FF00"
  },
  "components": {
    "global": {
      "backgroundColor": "#FF0000",
      "textColor": "#FFFFFF",
      "fontFamily": "Comic Sans MS, cursive",
      "borderRadius": "50px",
      "boxShadow": "0px 4px 20px rgba(255, 0, 0, 0.5)"
    },
    "header": {
      "backgroundColor": "#FF0000",
      "textColor": "#FFFFFF",
      "borderRadius": "50px",
      "backdropFilter": "blur(10px)",
      "border": "5px solid #00FF00",
      "fontFamily": "Comic Sans MS, cursive"
    },
    "buttons": {
      "backgroundColor": "#00FF00",
      "textColor": "#000000",
      "borderRadius": "50px",
      "fontSize": "20px",
      "fontWeight": "900",
      "transition": "all 0.2s ease"
    },
    "cards": {
      "backgroundColor": "#FF0000",
      "textColor": "#FFFFFF",
      "borderRadius": "50px",
      "border": "5px solid #00FF00",
      "fontFamily": "Comic Sans MS, cursive"
    },
    "navigation": {
      "backgroundColor": "#FF0000",
      "textColor": "#FFFFFF",
      "borderRadius": "50px",
      "backdropFilter": "blur(10px)",
      "border": "5px solid #00FF00",
      "fontFamily": "Comic Sans MS, cursive"
    },
    "containers": {
      "backgroundColor": "#FF0000",
      "textColor": "#FFFFFF",
      "borderRadius": "50px",
      "fontFamily": "Comic Sans MS, cursive"
    }
  },
  "tokenColors": {
    "positive": "#00FF00",
    "negative": "#FF0000", 
    "neutral": "#FFFF00",
    "warning": "#FF00FF",
    "info": "#00FFFF"
  },
  "transitions": {
    "default": "all 0.5s ease",
    "hover": "all 0.8s ease",
    "focus": "all 0.3s ease"
  }
}